[launchers]
  # command can be an uicb function or an uicb function + extension (see example)
  # Example of uicb + ext:
  #   command = "spawn xterm -e"

  [launcher]
     name = "exec"
     prompt = " Run:"
     command = "spawn"
     width = 300
  [/launcher]

  [launcher]
     name = "launcher_man" 
     prompt = " Man:" 
     command = "spawn lxterminal -e man"
     width = 300
  [/launcher]
[/launchers]
